attributetype ( 1.3.6.1.4.1.47119.100.1.1.1.5
    NAME ( 'SciFiDeviceTags' 'SciFiDevTags')
    DESC 'SciFi Device Tags'
    EQUALITY caseIgnoreMatch
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 
    SINGLE-VALUE )

